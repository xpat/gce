server {
  server_name  www.pinchepoutine.mx pinchepoutine.mx;
  access_log  /var/log/nginx/pinchepoutinemx.access.log;
  error_log  /var/log/nginx/pinchepoutinemx.error.log;
location / {
    root  /var/www/pinchepoutinemx/dist;
    index  index.html index.htm;  

    }



    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/pinchepoutine.mx/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/pinchepoutine.mx/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot


}
server {
    if ($host = www.pinchepoutine.mx) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = pinchepoutine.mx) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


  listen  80;
  server_name  www.pinchepoutine.mx pinchepoutine.mx;
    return 404; # managed by Certbot




}